{
  "name": "nmap",
  "script_path": "scripts/nmap.sh",
  "description": "Run nmap scan on a target",
  "group": "Network",
  "parameters": [
    {
      "name": "Target",
      "required": true,
      "type": "text",
      "description": "IP or domain"
    },
    {
      "name": "Profiles",
      "same_arg_param": true,
      "type": "list",
      "description": "Predefined nmap profiles",
      "values": [
        "-A",
        "-p-",
        " --script=http-headers -p80,443"
      ]
    },
    {
      "name": "Ports",
      "param": "-p",
      "type": "text"
    }
  ]
}